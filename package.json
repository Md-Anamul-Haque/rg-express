{
  "private": false,
  "workspaces": [
    "packages/*"
  ],
  "version": "0.1.3",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "rg-express",
  "author": "md-anamul-haque",
  "bugs": {
    "url": "https://github.com/Md-Anamul-Haque/rg-express/issues"
  },
  "homepage": "https://github.com/Md-Anamul-Haque/rg-express#readme",
  "keywords": [
    "rg-express",
    "express",
    "filebaseRouting",
    "rg"
  ],
  "module": "dist/rg-express.esm.js",
  "size-limit": [
    {
      "path": "dist/rg-express.cjs.production.min.js",
      "limit": "300 KB"
    },
    {
      "path": "dist/rg-express.esm.js",
      "limit": "300 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^10.0.1",
    "@types/express": "^4.17.20",
    "husky": "^8.0.3",
    "size-limit": "^10.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}